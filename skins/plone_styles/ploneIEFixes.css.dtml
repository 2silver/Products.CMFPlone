/* <dtml-with stylesheet_properties>
** WinIE Fix stylesheet
*/
#documentContent {
    /* Fix for IE6 disappearing float bug */
    height: 100%;
}
.hiddenstructure {
    display: none;
}
div#columns div.box {
    text-align: left;
}
div#columns div#left,
div#columns div#right {
    overflow: hidden;
}
div.overflow {
    overflow: auto;
}

/* </dtml-with> */
