<html metal:use-macro="here/main_template/macros/master">
  <div metal:fill-slot="left_navigation">
     <metal:block tal:repeat="o python:here.contentValues(here.display_types)">
      <a href="#" 
         class="subNav"
         tal:content="o/title_or_id"
         tal:attributes="href o/absolute_url"> Gary Meshell Whips Ass </a> <br />
      </metal:block>
  </div>

  <div metal:fill-slot="main"
       tal:define="logo here/client_image | nothing; case_study here/case_study.pdf|nothing;" >

    <img src=""
         border="0"
         tal:attributes="src logo/absolute_url;
	                 usemap python:test(case_study, '#case_study', None);"
	 tal:condition="logo" /> 
    <map name="case_study"
         tal:condition="case_study">
      <area shape="rect" 
            coords="364,52,446,69" 
            href=""
	    target="top"
	    alt="view detail" 
	    title="view detail" 
	    tal:attributes="href string:${here/absolute_url}/case_study.pdf" />
    </map> 

    <span class="copy">
      <p tal:replace="structure here/CookedBody">
      This is the client information.
      </p>
    </span>
  </div>

</html>
