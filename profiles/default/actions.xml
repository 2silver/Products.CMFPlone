<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <action-provider name="portal_workflow"/>
 <action-provider name="portal_actions"/>
 <action-provider name="portal_types"/>
 <object name="document_actions" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="rss" meta_type="CMF Action">
   <property name="title">RSS feed of this listing</property>
   <property name="description"></property>
   <property name="url_expr">string:$object_url/RSS</property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">python:portal.portal_syndication.isSyndicationAllowed(object)</property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="sendto" meta_type="CMF Action">
   <property name="title">Send this page to somebody</property>
   <property name="description"></property>
   <property name="url_expr">string:$object_url/sendto_form</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Allow sendto"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="print" meta_type="CMF Action">
   <property name="title">Print this page</property>
   <property name="description"></property>
   <property name="url_expr">string:javascript:this.print();</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="addtofavorites" meta_type="CMF Action">
   <property name="title">Add to Favorites</property>
   <property name="description"></property>
   <property name="url_expr">string:${object_url}/addtoFavorites</property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">python:(member is not None) and (portal.portal_membership.getHomeFolder() is not None)</property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">False</property>
  </object>
  <object name="full_screen" meta_type="CMF Action">
   <property name="title">Toggle full screen mode</property>
   <property name="description"></property>
   <property
      name="url_expr">string:javascript:toggleFullScreenMode();</property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:member is not None</property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="extedit" meta_type="CMF Action">
   <property
      name="title">Edit this file in an external application (Requires Zope ExternalEditor installed)</property>
   <property name="description"></property>
   <property name="url_expr">string:$object_url/external_edit</property>
   <property name="icon_expr"></property>
   <property name="available_expr">object/externalEditorEnabled</property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
